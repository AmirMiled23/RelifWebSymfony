{% extends 'backbase.html.twig' %}

{% block title %}New Resource{% endblock %}

{% block body %}
    <h1>Create new Resource</h1>

    <form method="post" action="{{ path('app_resource_back_new') }}" enctype="multipart/form-data">
        {{ form_start(form) }}
        {{ form_row(form.type) }}
        {{ form_row(form.url) }}
        {{ form_row(form.description) }}
        {{ form_row(form.pdf) }}
        <button type="submit" class="btn btn-primary">Save</button>
        {{ form_end(form) }}
    </form>

    <a href="{{ path('app_resource_back_index') }}">back to list</a>
{% endblock %}
